        SYSTEM_RESET                    =>      gt3_rx_system_reset_c,
        ERROR_COUNT_OUT                 =>      gt3_error_count_i,
        TRACK_DATA_OUT                  =>      gt3_track_data_i
    );




TRACK_DATA_OUT                               <= track_data_out_i;

track_data_out_i                             <= 
                                gt0_track_data_i  and
                                gt1_track_data_i  and
                                gt2_track_data_i  and
                                gt3_track_data_i ;














